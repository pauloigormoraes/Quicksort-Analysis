#include <stdio.h>
#include <stdlib.h>
#include <time.h>
#define DP 10

int f_existe (int *d, int r) {
  int i;
  for(i = 0; i < DP; i++) {
   if (d[i] == r) return 1;
  }
  return 0;
}

int f_outrovalor(int *d) {
  int r;
  srand((unsigned int)time((time_t *)NULL));
  do {
      r = 1 + (rand() % 10);
  }
  while (f_existe(d, r));
  return r;
}


